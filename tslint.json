{
  "extends": [
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "dist/src"
  ],
  "rules": {
    "no-unused-variable": true,
    "no-console": [true, "log", "error"],
    "quotemark": [true, "double", "avoid-escape"],
    "no-var-keyword": true,
    "indent": [true, "tabs"],
    "prefer-const": true,
    "no-duplicate-imports": true
  }
}
